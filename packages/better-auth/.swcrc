{
	"$schema": "https://json.schemastore.org/swcrc",
	"sourceMaps": true,
	"jsc": {
		"target": "es2022",
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": true
		},
		"transform": {
			"react": {
				"runtime": "automatic",
				"pragmaFrag": "React.Fragment",
				"throwIfNamespace": true,
				"development": false,
				"useBuiltins": true
			}
		},
		"baseUrl": ".",
		"paths": {
			"@payload-auth/better-auth": ["./src"],
			"@payload-auth/better-auth/*": ["./src/*"]
		},
		"loose": false,
		"externalHelpers": false
	},
	"module": {
		"type": "es6"
	},
	"minify": false,
	"isModule": true
}
